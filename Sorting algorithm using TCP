import java.net.*;
import java.io.*;
import java.util.*;

class Server {
	public static void main(String[] args) {
		try {
			ServerSocket ssoc = new ServerSocket(8888);
			Socket soc = ssoc.accept();

			ObjectOutputStream oos = new ObjectOutputStream(soc.getOutputStream());
			ObjectInputStream ois = new ObjectInputStream(soc.getInputStream());

			ArrayList<Integer> arrayList = (ArrayList<Integer>) ois.readObject();

			Collections.sort(arrayList);
			
			oos.writeObject(arrayList);
			oos.flush();


		} catch (Exception e) {
			System.out.println("Error!");
		}
	}
}

class Client {
	public static void main(String[] args) {
		try {
			Socket socket = new Socket("localhost", 8888);
			ObjectOutputStream oos = new ObjectOutputStream(socket.getOutputStream());
			ObjectInputStream ois = new ObjectInputStream(socket.getInputStream());

			Scanner input = new Scanner(System.in);

			System.out.print("Number of elements: ");
			int num = Integer.parseInt(input.nextLine());

			ArrayList<Integer> arrayList = new ArrayList<Integer>(num);
			
			for (int i = 0; i < num; i++) {
				arrayList.add(Integer.parseInt(input.nextLine()));
			}
			
			oos.writeObject(arrayList);
			oos.flush();

			ArrayList<Integer> sortedList = (ArrayList<Integer>) ois.readObject();
			System.out.println("Sorted numbers:" + sortedList);

			socket.close();
		} catch (Exception e) {
			System.out.println("Error!");
		}
	}
}
